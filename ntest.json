{
    "$schema": "node_modules/@kayahr/ntest/ntest.schema.json",
    "files": "lib/test/**/*.test.js",
    "reporter": [
        { "name": "spec" },
        { "name": "lcov", "destination": "lib/test/lcov.info" }
    ],
    "coverage": {
        "enabled": true
    },
    "sourceMaps": true,
    "exposeGC": true,
    "import": [ "./lib/test/dom.js" ]
}
